{% extends '@bootstrap5/base.html.twig' %}

{% block body %}
    <div class="container">
        <table id="table" class="table table-striped table-bordered align-middle">
            <thead>
                <tr>
                    <th>Date</th>
                    <th>Class</th>
                    <th>Method</th>
                    <th>Documentation</th>
                    <th>Tags</th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
    <script>
        var table = $('#table').DataTable({{ get_table_config() }});
        table.rows.add({{ get_table_rows(annotations) }}).draw();
    </script>
{% endblock body %}